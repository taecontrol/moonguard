{
    "name": "taecontrol/larastats",
    "description": "Website monitoring platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taecontrol\\Larastats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taecontrol\\Larastats\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "spatie/url": "^2.1",
        "laravel/slack-notification-channel": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.13"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taecontrol\\Larastats\\LarastatsServiceProvider"
            ]
        }
    }
}
