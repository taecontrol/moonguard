{
    "name": "taecontrol/moonguard",
    "description": "Website monitoring platform",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "moonguard",
        "monitoring",
        "exceptions",
        "report", 
        "web"
    ],
    "homepage": "https://github.com/taecontrol/moonguard",
    "autoload": {
        "psr-4": {
            "Taecontrol\\MoonGuard\\": "src/",
            "Taecontrol\\MoonGuard\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taecontrol\\MoonGuard\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "spatie/url": "^2.1",
        "laravel/slack-notification-channel": "^2.4",
        "spatie/ssl-certificate": "^2.4",
        "filament/filament": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.13",
        "spatie/laravel-ray": "^1.31",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taecontrol\\MoonGuard\\Providers\\MoonGuardServiceProvider",
                "Taecontrol\\MoonGuard\\Providers\\MoonGuardFilamentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Luis GÃ¼ette",
            "role": "Developer"
        },
        {
            "name": "Ronald Pereira",
            "role": "Developer"
        },
        {
            "name": "Alexis Fraudita",
            "role": "Developer"
        },
        {
            "name": "Wilson Velasco",
            "role": "Developer"
        }
    ]
}